[config]
SCM_DO_BUILD_DURING_DEPLOYMENT=true
SCM_DO_BUILD_DURING_DEPLOYMENT_DELAY=30
SCM_USE_MSDEPLOY=true
WEBSITE_PYTHON_ENABLE_GUNICORN_BIND=0.0.0.0:8181
WEBSITE_PYTHON_ENABLE_GUNICORN=true
WEBSITE_PYTHON_ENABLE_WORKER_COUNT=4
WEBSITE_PYTHON_ENABLE_WORKER_TIMEOUT=120
WEBSITE_PYTHON_ENABLE_WORKER_PRELOAD=true
WEBSITE_PYTHON_ENABLE_WORKER_ACCESS_LOG=true
WEBSITE_PYTHON_ENABLE_WORKER_ERROR_LOG=true
WEBSITE_PYTHON_ENABLE_WORKER_CAPTURE_OUTPUT=true
WEBSITE_PYTHON_ENABLE_WORKER_STDIO_INHERITANCE=true
WEBSITE_PYTHON_ENABLE_WORKER_LOG_LEVEL=debug

[deployment]
command = bash startup.sh